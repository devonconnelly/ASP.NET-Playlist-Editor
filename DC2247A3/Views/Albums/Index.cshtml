@model IEnumerable<DC2247A3.Models.AlbumBaseViewModel>

@{
    ViewBag.Title = "Albums";
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>

    </tr>
}

</table>
